  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/mocha": "^7.0.2",
    "@types/mongodb": "^3.5.14",
    "@types/node": "^13.13.4",
    "@types/node-fetch": "^2.5.7",
    "@types/uuid": "^7.0.3",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
